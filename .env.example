# Supabase Configuration
# Public keys (safe to expose to client)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Server-only keys (NEVER expose to client - do NOT prefix with NEXT_PUBLIC_)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# FACEIT API Configuration
NEXT_PUBLIC_FACEIT_API_TOKEN=your_faceit_api_token_here
FACEIT_CLIENT_ID=your_faceit_client_id_here
FACEIT_CLIENT_SECRET=your_faceit_client_secret_here

# NextAuth Configuration
NEXTAUTH_SECRET=your_nextauth_secret_here
